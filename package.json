{
  "name": "multi_chat",
  "version": "1.0.0",
  "description": "",
  "browserslist": "> 0.5%, last 2 versions",
  "scripts": {
    "start": "node server.js",
    "start:dev": "parcel static/*.html -p 3000 --no-source-maps",
    "clean": "rimraf ./dist",
    "copy:css": "copyfiles src/**/*.css -f ./dist/styles/",
    "build:html": "parcel build static/index.html --no-cache --no-source-maps",
    "build:ts": "npm run clean && tsc --build ./tsconfig.json",
    "build": "npm run build:ts && npm run copy:css && npm run build:html",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EgorEres/multi_chat.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EgorEres/multi_chat/issues"
  },
  "homepage": "https://github.com/EgorEres/multi_chat#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-handlebars-precompile": "^1.0.2",
    "postcss": "^8.3.6",
    "postcss-modules": "^3.2.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "express": "^4.17.1",
    "handlebars": "^4.7.7"
  },
  "alias": {
    "styles": "./dist/styles/"
  },
  "main": "index.js",
  "keywords": []
}
